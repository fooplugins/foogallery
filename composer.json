{
  "name": "fooplugins/foogallery",
  "description": "FooGallery is the most intuitive and extensible gallery management tool ever created for WordPress",
  "minimum-stability": "stable",
  "license": "GPL 3.0",
  "authors": [
    {
      "name": "FooPlugins",
      "email": "brad@fooplugins.com"
    }
  ],
  "autoload": {
      "psr-4": {
        "FooPlugins\\FooGallery\\": "includes",
        "FooPlugins\\FooGallery\\Pro\\": "Pro",
        "FooPlugins\\FooGallery\\Extensions\\": "Extensions",
        "FooPlugins\\FooGallery\\Gutenberg\\": "gutenberg"
      },
      "classmap": [
        "includes",
        "Pro",
        "Extensions",
        "gutenberg"
      ]
    },
    "extra": {
        "installer-paths": {
            "wp-content/plugins/{$name}/": ["type:wordpress-plugin"]
        }
    },
    "require-dev": {
    "squizlabs/php_codesniffer": "3.*",
    "wp-coding-standards/wpcs": "2.*"
    },
    "config": {
    "allow-plugins": {
        "composer/installers": true
    }
    },
    "require": {
        "composer/installers": "^1.12",
        "php": ">=5.6.0"
    }
}
